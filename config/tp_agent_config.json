{
  "max_rounds": 20,
  "execution": {
    "default_timeout": 10,
    "python_timeout": 10,
    "mathematica_timeout": 30,
    "python_memory_limit": 536870912
  },
  "providers": {
    "openai": {
      "base_url": "https://api.openai.com/v1",
      "model": "gpt-5",
      "max_output_tokens": 200000,
      "temperature": 0.7,
      "timeout_sec": 1000,
      "reasoning": {
        "effort": "medium"
      }
    }
  },
  "paths": {
    "system_prompt": "config/system_prompt.txt",
    "api_key_file": "config/api_key.json",
    "wolfram_state_file": "~/.tp_agent/wolfram_state.json"
  },
  "wolfram": {
    "use_docker": true,
    "docker_image": "wolframresearch/wolframengine",
    "container_name": "tp-wolfram-engine",
    "activation_timeout": 60
  },
  "output": {
    "default_dir": "outputs",
    "save_json": true,
    "save_log": true,
    "quiet_mode": false
  }
}